            <title>{% block title %}Welcome!{% endblock %}</title>
            <link href="https://fonts.googleapis.com/css?family=Playfair+Display:700,900" rel="stylesheet">
            {% block stylesheets %}
                <link rel="stylesheet" href="{{ asset('build/app.css') }}">
            {% endblock %}
        </head>
    <body>
